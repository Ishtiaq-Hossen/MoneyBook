<AnchorPane id="AnchorPane" prefHeight="662.0" prefWidth="739.0" style="-fx-background-color: #e9e7e7;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.ProfileController">
   <children>
      <Label layoutX="68.0" layoutY="60.0" prefHeight="66.0" prefWidth="215.0" text="Profile Settings">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
         <effect>
            <Blend />
         </effect>
      </Label>
      <Label layoutX="66.0" layoutY="136.0" prefHeight="39.0" prefWidth="125.0" text="Profile details">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="81.0" layoutY="210.0" text="Name" />
      <TextField fx:id="nameTextField" layoutX="265.0" layoutY="194.0" prefHeight="31.0" prefWidth="380.0" style="-fx-border-width: 0 0 2 0; -fx-border-color: black; -fx-background-color: none;" />
      <Label layoutX="81.0" layoutY="269.0" text="Username" />
      <DatePicker fx:id="dateOfBirthDatePicker" layoutX="265.0" layoutY="361.0" prefHeight="31.0" prefWidth="380.0" promptText="dd/yy/month" style="-fx-background-color: white; -fx-border-color: black; -fx-border-width: 0 0 2 0;" />
      <TextField fx:id="userNameTextField" layoutX="265.0" layoutY="252.0" prefHeight="31.0" prefWidth="380.0" style="-fx-background-color: none; -fx-border-width: 0 0 2 0; -fx-border-color: black;" />
      <Label layoutX="81.0" layoutY="369.0" text="Date of birth" />
      <Label layoutX="81.0" layoutY="322.0" text="Password" />
      <TextField fx:id="passswordTextField" layoutX="265.0" layoutY="305.0" prefHeight="31.0" prefWidth="380.0" style="-fx-background-color: none; -fx-border-width: 0 0 2 0; -fx-border-color: black;" />
      <Label layoutX="68.0" layoutY="423.0" prefHeight="39.0" prefWidth="125.0" text="Contacts">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="81.0" layoutY="489.0" text="Phone" />
      <TextField fx:id="phoneTextField" layoutX="265.0" layoutY="473.0" prefHeight="31.0" prefWidth="380.0" style="-fx-border-width: 0 0 2 0; -fx-border-color: black; -fx-background-color: none;" />
      <Label layoutX="81.0" layoutY="537.0" text="Email" />
      <TextField fx:id="emailTextField" layoutX="265.0" layoutY="521.0" prefHeight="31.0" prefWidth="380.0" style="-fx-border-width: 0 0 2 0; -fx-border-color: black; -fx-background-color: none;" />
      <Button layoutX="333.0" layoutY="593.0" mnemonicParsing="false" onMouseClicked="#saveProfileOnClick" prefHeight="50.0" prefWidth="101.0" style="-fx-background-color: white;" text="Save">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <ImageView fx:id="imageView" fitHeight="149.0" fitWidth="158.0" layoutX="540.0" layoutY="26.0" pickOnBounds="true">
         <effect>
            <DropShadow />
         </effect></ImageView>
      <Button layoutX="413.0" layoutY="27.0" mnemonicParsing="false" onMouseClicked="#updateImageButtonOnClick" style="-fx-background-color: white;" text="Update Image">
         <effect>
            <DropShadow />
         </effect>
      </Button>
   </children>
</AnchorPane>
